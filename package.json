{
	"name": "gatsby-ts-tailwind-starter",
	"description": "A Typescript starter with for Gatsby with Tailwind and Hot-reloading",
	"version": "1.0.0",
	"private": true,
	"author": "Oliver Heward <oliver@hewy.dev> (https://hewy.dev)",
	"license": "MIT",
	"keywords": [
		"gatsby",
		"starter",
		"typescript"
	],
	"scripts": {
		"develop": "gatsby develop",
		"start": "gatsby develop",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"type-check": "tsc --noEmit",
		"lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
		"format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
	},
	"lint-staged": {
		"./src/**/*.{ts,js,jsx,tsx}": [
			"eslint --ignore-path .gitignore --fix",
			"prettier --ignore-path .gitignore --write"
		]
	},
	"dependencies": {
		"classnames": "2.3.2",
		"dotenv": "16.0.2",
		"gatsby": "4.17.1",
		"gatsby-plugin-gdpr-cookies": "2.0.9",
		"gatsby-plugin-react-helmet": "5.23.0",
		"gatsby-plugin-sass": "5.17.0",
		"gatsby-source-filesystem": "4.23.0",
		"js-cookie": "3.0.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-helmet": "6.1.0",
		"react-inlinesvg": "3.0.1",
		"sass": "1.53.0",
		"tsconfig-paths-webpack-plugin": "3.5.2"
	},
	"devDependencies": {
		"@babel/preset-react": "7.17.12",
		"@babel/preset-typescript": "7.17.12",
		"@types/node": "16.11.41",
		"@types/react": "18.0.14",
		"@types/react-dom": "18.0.5",
		"@types/tailwindcss": "3.0.11",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"autoprefixer": "10.4.7",
		"babel-preset-gatsby": "2.17.0",
		"eslint": "8.18.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-standard": "17.0.0",
		"eslint-import-resolver-typescript": "2.7.1",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.6.0",
		"eslint-plugin-n": "15.2.3",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-promise": "6.0.0",
		"eslint-plugin-react": "7.30.1",
		"eslint-plugin-react-hooks": "4.6.0",
		"gatsby-plugin-postcss": "5.17.0",
		"postcss": "8.4.14",
		"prettier": "2.7.1",
		"tailwindcss": "3.1.4",
		"typescript": "4.7.4"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}
